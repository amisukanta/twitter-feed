<h1>Tweet Feeds </h1>
  <%= will_paginate %>
<div class="row">

	<% @feeds.each do |feed| %>
    <div class="col-xs-12 col-md-8">
	  @<%= feed.screen_name %> 
	  <span> | <small> Feed time: <i> <%= feed.created_at %></i></small></span>
	  <%= simple_format(feed.content) %>
	  
	  </div>
	<% end %>

</div>
	<%= will_paginate %>
<%#= @tweets %>